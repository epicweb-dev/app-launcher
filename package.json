{
  "type": "module",
  "name": "@epic-web/app-launcher",
  "version": "0.2.0",
  "description": "Utility for launching your applications on a per-test basis.",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "dev": "npm run build -- --watch",
    "test": "vitest",
    "build": "tsc -p ./tsconfig.build.json",
    "release": "release publish"
  },
  "files": [
    "./src",
    "./build"
  ],
  "keywords": [
    "launcher",
    "app",
    "test",
    "rsc",
    "utility",
    "helper"
  ],
  "author": "Artem Zakharchenko <kettanaito@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@ossjs/release": "^0.10.0",
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@epic-web/invariant": "^1.0.0",
    "@open-draft/deferred-promise": "^2.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
